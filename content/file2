135     var bIsUc = sUserAgent.match(/ucweb/i) == "ucweb";  
136     var bIsAndroid = sUserAgent.match(/android/i) == "android";  
137     var bIsCE = sUserAgent.match(/windows ce/i) == "windows ce";  
138     var bIsWM = sUserAgent.match(/windows mobile/i) == "windows mobile";  
139     if(bIsIphoneOs || bIsAndroid||bIsMidp||bIsUc7||bIsUc||bIsCE||bIsWM){  
