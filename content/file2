429 									 $("#fail").show();
430 									 setTimeout("$('#fail').hide();", 5000);
431 									 setTimeout("$('#postReply').show();", 5000);
432 								 }
433 							}
434 						});	 
435 					}
436 				   return false; 
437 			   });
