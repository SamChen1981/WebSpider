149 function fGetQuery(name){
150     var sUrl = window.location.search.substr(1);  
151     var r = sUrl.match(new RegExp("(^|&)" + name + "=([^&]*)(&|$)"));  
152     return (r == null ? null : unescape(r[2]));  
153 }  
