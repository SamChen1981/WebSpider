412 								 $("#postReply").hide();
413 								 $("#messageContent").val("");
414 								 $(".post0").attr("href",obj.postLink0);
415 								 $(".post0").html(obj.postTitle0);
416 								 $(".post1").attr("href",obj.postLink1);
417 								 $(".post1").html(obj.postTitle1);
418 								 if(obj.msg=='success'&& obj.quickPost==1){
419 									 $("#randomUserName").html(obj.userName);
420 									 $("#randomUserPassword").html(obj.password);
421 									 $("#quickPostSuccess").show();
422 									 setTimeout(BtnCount, 1000);
423 								 }else if(obj.msg=='success'){
424 									 $("#success").show();
425 									 setTimeout("$('#success').hide();", 5000);
426 									 setTimeout("$('#postReply').show();", 5000);									 
427 								 }else{
